{
  "name": "n8n-nodes-oracle-database-advanced",
  "version": "2.0.0",
  "description": "Oracle Database node com recursos avançados para cargas pesadas e Oracle 19c+",
  "keywords": [
    "n8n-community-node-package",
    "oracle",
    "database",
    "advanced",
    "plsql",
    "bulk-operations",
    "oracle-19c",
    "connection-pool",
    "transactions"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jonales/n8n-nodes-oracle-database-advanced",
  "author": {
    "name": "Jônatas Meireles Sousa Vieira",
    "email": "jonatas.mei@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonales/n8n-nodes-oracle-database-advanced.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Oracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Oracle/OracleDatabaseAdvanced.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/oracledb": "^6.3.0",
    "@types/request-promise-native": "~1.0.21",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "^1.31.0",
    "n8n-workflow": "^1.40.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslint": "^6.1.3",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "oracledb": "^6.9.0"
  }
