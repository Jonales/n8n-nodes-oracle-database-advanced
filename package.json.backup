{
  "name": "n8n-nodes-oracle-database-advanced",
  "version": "2.0.1",
  "description": "Oracle Database node com recursos avançados para cargas pesadas e Oracle 19c+",
  "keywords": [
    "n8n-community-node-package",
    "oracle",
    "database",
    "advanced",
    "plsql",
    "bulk-operations",
    "oracle-19c",
    "connection-pool",
    "transactions"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jonales/n8n-nodes-oracle-database-advanced",
  "author": {
    "name": "Jônatas Meireles Sousa Vieira",
    "email": "jonatas.mei@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonales/n8n-nodes-oracle-database-advanced.git"
  },

  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "README.md", "LICENSE"],
  "sideEffects": false,
  "engines": {
    "node": ">=18.18"
  },
  "packageManager": "npm@10",

  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts,.js",
    "lintfix": "eslint . --ext .ts,.js --fix",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm test -- --runInBand",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },

  "publishConfig": {
    "access": "public",
    "provenance": true
  },

  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Oracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Oracle/OracleDatabaseAdvanced.node.js"
    ]
  },

  "dependencies": {
    "oracledb": "^6.9.0"
  },

  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.2.1",
    "@types/oracledb": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.6.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "^1.105.0",
    "n8n-workflow": "^1.105.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}